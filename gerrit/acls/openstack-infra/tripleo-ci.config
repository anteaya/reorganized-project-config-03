[access "refs/heads/*"]
abandon = group toci-core
label-Code-Review = -2..+2 group toci-core
label-Workflow = -1..+1 group toci-core

[access "refs/heads/proposed/*"]
abandon = group toci-core
label-Code-Review = -2..+2 group toci-core
label-Workflow = -1..+1 group toci-core

[receive]
requireChangeId = true
requireContributorAgreement = true

[submit]
mergeContent = true
